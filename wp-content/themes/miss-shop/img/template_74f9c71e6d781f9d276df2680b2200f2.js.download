function icon_position(element) {
	var top_const = 4;
	var right_const = 8;
	$(element).each(function() {
		var h_prop = $(this).outerHeight();
		var w_prop = $(this).outerWidth();
		var top_prop = ((w_prop - h_prop) / 2) + top_const;
		var right_prop = ((h_prop - w_prop) / 2) + right_const;
		$(this).css({
			'top': top_prop,
			'right': right_prop
		});
	});
};

