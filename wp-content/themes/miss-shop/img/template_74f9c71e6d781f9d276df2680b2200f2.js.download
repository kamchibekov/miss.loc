(function( window, document, $, undefined) {  
    window.msFeedbackDemoPhoneDetail = function(arParams)
    {
        this.destroy();
        this.init();
    };

    window.msFeedbackDemoPhoneDetail.prototype.init = function()
    {                                                                                                                                                      
        this.maska = "(999) 999-99-99";
        this.maska = $.trim(this.maska);
        if(this.maska!="")
            $(".sotbit_feedback_demo_phone form input[name='feedback_demo_phone']").mask(this.maska, {placeholder:"_"});
        /*kostyl for safari*/    
        $(document).on("mouseenter", ".sotbit_feedback_demo_phone form", this, this.safariMask);
        /**/
        $(document).on("submit", ".sotbit_feedback_demo_phone form", this, this.submitOrderPhone);
    };
    window.msFeedbackDemoPhoneDetail.prototype.destroy = function()
    { 
        $(document).off("submit", ".sotbit_feedback_demo_phone form", this.submitOrderPhone);            
    };
    window.msFeedbackDemoPhoneDetail.prototype.safariMask = function(e)
    {
        if($.browser.safari)
        {
            _this = e.data;
            if(_this.maska!="")
            {
                $(".sotbit_feedback_demo_phone form input[name='feedback_demo_phone']").mask(_this.maska, {placeholder:"_"});
            }
        }      
    }
    window.msFeedbackDemoPhoneDetail.prototype.submitOrderPhone = function(e)
    {
        e.preventDefault();
        //BX.showWait();
        _this = e.data;
        
        v = "(999) 999-99-99";
        v = $.trim(v);
        req = _this.strReplace(v);
        var this_cont = $(this);
        v = $(this).find("input[type='text']").val();                                      
        //BX.closeWait();                                                                          
        if(v.search(req)!=-1 && $(this).find("input[name='domain']").val()!='')
        {                           
            $(this).removeClass("error_input");                    
            var formData = encodeURIComponent(JSON.stringify({
            'domain':$(this).find("input[name='domain']").val(), 
            'phone':'+7'+$(this).find("input[name='feedback_demo_phone']").val(),
            'user_city':$(this).find("input[name='user_city']").val(), 
            'user_came':$(this).find("input[name='user_came']").val(), 
            'user_came_where':$(this).find("input[name='user_came_where']").val(), 
            }));                     
                 /*
            var XHR = ("onload" in new XMLHttpRequest()) ? XMLHttpRequest : XDomainRequest;
            var xhr = new XHR();
            // (2)     :)                            
            xhr.open('POST', $(this).attr("action"), true);
            xhr.setRequestHeader('Content-type', 'application/json; charset=utf-8');
            xhr.onload = function() {
                console.log('onload');
            }    
            xhr.onerror = function() {
                console.log( ' ' + this.status );
            }
            xhr.send('json='+formData); 
                */
           var contentType ="application/x-www-form-urlencoded; charset=utf-8";
           if(window.XDomainRequest) //for IE8,IE9
                contentType = "text/plain";  
              
            $.ajax({                                    
                url: $(this).attr("action")+'?callback=?',
                data:'jsonData=' + formData,  
                type: 'GET',            
                dataType: "jsonp",    
                success: function (data) {                  
                    console.log(data.status);
                    $(".block_demo_feedback.sotbit_feedback_demo_phone form.sotbit_feedback_demo_phone_form").empty().append('<div class="success_send_feedback"><span>.     .</span></div>');
                    },
                error: function (data) {     
                    console.log(data.status);                                                
                    }
            }); 
        }else{                   
            $(this).addClass("error_input");                                    
        }
        BX.closeWait();
        return false;
    };
    
    window.msFeedbackDemoPhoneDetail.prototype.strReplace = function(str)
    {
        str = str.replace("+", "\\+");
        str = str.replace("(", "\\(");
        str = str.replace(")", "\\)");
        str = str.replace(/[0-9]/g, "[0-9]{1}");
        return new RegExp(str, 'g');;

    };
})(window, document, jQuery);
